    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "jest-html-reporters": "^3.1.4",
    "loki": "^0.32.0",
    "mini-css-extract-plugin": "^2.7.6",
    "reg-cli": "^0.17.7",
    "sass": "^1.64.1",
    "sass-loader": "^13.3.2",
    "storybook": "^7.1.1",
    "style-loader": "^3.3.3",
